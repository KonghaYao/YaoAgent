{
    "name": "@langgraph-js/sdk",
    "version": "3.0.1",
    "description": "The UI SDK for LangGraph - seamlessly integrate your AI agents with frontend interfaces",
    "main": "dist/index.js",
    "type": "module",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "exports": {
        "./react": {
            "import": "./dist/react/index.js",
            "types": "./dist/react/index.d.ts"
        },
        "./vue": {
            "import": "./dist/vue/index.js",
            "types": "./dist/vue/index.d.ts"
        },
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "prepublish": "pnpm build"
    },
    "keywords": [
        "langgraph",
        "ui",
        "sdk",
        "frontend",
        "ai",
        "agents",
        "langchain",
        "react",
        "vue"
    ],
    "author": "LangGraph Team",
    "license": "Apache-2.0",
    "packageManager": "pnpm@10.6.3",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/KonghaYao/YaoAgent.git"
    },
    "homepage": "https://langgraph-js.netlify.app",
    "bugs": {
        "url": "https://github.com/KonghaYao/YaoAgent/issues"
    },
    "dependencies": {
        "@langchain/langgraph-sdk": "^0.1.10",
        "@langgraph-js/pure-graph": "^1.5.2",
        "eventemitter3": "^5.0.1",
        "jsonrepair": "^3.12.0",
        "nanostores": "^1.0.1",
        "ts-debounce": "^4.0.0",
        "zod": "^3.25.17",
        "zod-to-json-schema": "^3.24.5"
    },
    "peerDependencies": {
        "@nanostores/react": "^1.0.0",
        "@nanostores/vue": "^1.0.0",
        "react": "^19||^18",
        "vue": "^3.0.0"
    },
    "peerDependenciesMeta": {
        "@nanostores/react": {
            "optional": true
        },
        "@nanostores/vue": {
            "optional": true
        },
        "react": {
            "optional": true
        },
        "vue": {
            "optional": true
        }
    },
    "devDependencies": {
        "@types/react": "^19.1.6",
        "vue": "^3.5.14"
    }
}
